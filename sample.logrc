[log]
log_dir=$HOME/Dropbox/docs/log/
snippet_dir=$HOME/Dropbox/docs/log/snippets/
editor=vim -c 'set tw=75'
alternate_editor=emacsclient -t
line_length=75
#tags_off=true

[tags]
comment=[33m
date=[1;34m
t=[1;32m
s=[32m
h=[1;33m
d=[1;31m
e=[31m
c=[36m
o=[1;35m
p=[36m
x=[1;35m
n=[31m

[extensions]
T=training
J=journal
M=meditation
